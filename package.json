{
  "name": "screen-qrcode-parser",
  "version": "0.0.1",
  "description": "识别屏幕中的二维码",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=dev electron index.js",
    "build": "electron-builder"
  },
  "author": "qwertyyb <qwertyyb@qq.com>",
  "license": "ISC",
  "build": {
    "appId": "cn.qwertyyb.screen-qrcode-parser",
    "mac": {
      "asar": true,
      "category": "public.app-category.utilities",
      "icon": "assets/appicon.png",
      "identity": null,
      "electronLanguages": [
        "en",
        "zh_CN"
      ],
      "extendInfo": {
        "LSBackgroundOnly": 1,
        "LSUIElement": 1
      }
    }
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "electron": "^6.1.3",
    "electron-builder": "^22.1.0"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "jsqr": "^1.2.0"
  }
}
